	# Get the Serial Number of the computer the script is acting on.
mySN=`/usr/sbin/ioreg -l | /usr/bin/awk '/IOPlatformSerialNumber/ { print $4;}' | /usr/bin/sed s/\"//g`

	# Loop through the array and find the entry.
for i in "${myList[@]}"; do
        myResult=$( /bin/echo -n ${i} | /usr/bin/egrep -iw ${mySN} )
        if [[ $myResult != "" ]]; then
                name=`/bin/echo ${myResult} | /usr/bin/cut -d "," -f 1`
                SN=`/bin/echo ${myResult} | /usr/bin/cut -d "," -f 2`
                ARD2=`/bin/echo ${myResult} | /usr/bin/cut -d "," -f 3`
                lcName=`/bin/echo ${name} | /usr/bin/tr '[:upper:]' '[:lower:]'`
                org=`/bin/echo "${myResult}" | /usr/bin/cut -d "," -f 5`
        fi
done

	# Should the computer name be empty, set the name to the serial number.
if [ -z ${name} ]; then
	/bin/echo -n "Error! Can't find the information in the array.          Setting name to: " >> /Library/Logs/DSE-RenameComputer.log
	name=${mySN}
	/bin/echo ${name} >> /Library/Logs/DSE-RenameComputer.log
else
	/bin/echo "         Setting name to: ${name}" >> /Library/Logs/DSE-RenameComputer.log
fi

	# Should the serial number be empty, set the serial number to the internal one. Kinda redundant here.
if [ -z ${SN} ]; then
	echo -n "Error! Can't find the information in the array. Setting Serial Number to: " >> /Library/Logs/DSE-RenameComputer.log
	SN=${mySN}
	/bin/echo ${SN} >> /Library/Logs/DSE-RenameComputer.log
else
	/bin/echo "Setting Serial Number to: ${SN}" >> /Library/Logs/DSE-RenameComputer.log
fi

	# Should the asset tag information be missing, then set the asset tag to Asset-N/A.
if [ -z ${ARD2} ]; then
	/bin/echo -n "Error! Can't find the information in the array.     Setting Asset Tag to: " >> /Library/Logs/DSE-RenameComputer.log
	ARD2="Asset-N/A"
	/bin/echo ${ARD2} >> /Library/Logs/DSE-RenameComputer.log
else
	/bin/echo "    Setting Asset Tag to: ${ARD2}" >> /Library/Logs/DSE-RenameComputer.log
fi

	# Set the ARD Info 1 to the Serial Number and Info 2 to the asset tag.
/usr/bin/defaults write /Library/Preferences/com.apple.RemoteDesktop Text1 "${SN}"
/usr/bin/defaults write /Library/Preferences/com.apple.RemoteDesktop Text2 "${ARD2}"
	# Rename the computer.
/usr/sbin/scutil --set ComputerName "${name}"
/usr/sbin/scutil --set HostName "${name}"
/usr/sbin/scutil --set LocalHostName "${lcName}"

	# If the computer is in the SINC organization, then run the SINC-InstallRootAppsScripts, and then the SINC-InstallBaseApplications.
if [ ${org} = "SINC" ]; then
	/bin/echo "Firing off Jamf Policies: SINC - Install Root Applications and Scripts, and SINC - Install Base Applications." >> /Library/Logs/DSE-RenameComputer.log
	/usr/local/jamf/bin/jamf policy -trigger "SINC-InstallRootAppsScripts"
	/usr/local/jamf/bin/jamf policy -trigger "SINC-InstallBaseApplications"
	# If the computer is in the CE organization, then run the STAFF-InstallRootAppsScripts and then the STAFF-InstallBaseApplications.
elif [ ${org} = "CE" ]; then
	/bin/echo "Firing off Jamf Policies: STAFF - Install Root Applications and Scripts, and STAFF - Install Base Applications." >> /Library/Logs/DSE-RenameComputer.log
	/usr/local/jamf/bin/jamf policy -trigger "STAFF-InstallRootAppsScripts"
	/usr/local/jamf/bin/jamf policy -trigger "STAFF-InstallBaseApplications"	
fi

exit 0


